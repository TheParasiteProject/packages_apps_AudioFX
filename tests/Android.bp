android_test {
    name: "AudioFXTests",

    srcs: ["**/*.java"],

    instrumentation_for: "AudioFX",

    static_libs: [
        "androidx.core_core",
        "androidx.test.ext.junit",
        "androidx.test.rules",
        "audiofx-android-support-test",
    ],

    optimize: {
        enabled: false,
    },

    platform_apis: true,
}

java_import {
    name: "audiofx-android-support-test",
    jars: ["lib/rules-0.3-release.jar"],
}
